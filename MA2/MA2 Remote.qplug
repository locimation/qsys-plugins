    local arr = {};
    for i=1,n do table.insert(arr, tostring(i * 5)); end;
    return arr;
  end;

  return {
    {
      Name = 'Command Count',
      Type = 'integer',
      Min = 2,
      Max = 100,
      Value = 8
    },{
      Name = 'Fader Count',
      Type = 'enum',
      Choices = fives(4),
      Value = '10'
    },{
      Name = 'Button Count',
      Type = 'enum',
      Choices = fives(16),
      Value = '10'
    },{
      Name = 'Verbose Debug',
      Type = 'boolean',
      Value = false
    }
  };
end;  

